	<queryString>
		<![CDATA[SELECT tanggal_penjualan,penjualan.id_barang,nama_barang,harga_beli_barang,jenis_penjualan,
harga_jual AS harga_jual_barang,jumlah ,harga_jual*jumlah AS total,
harga_jual*jumlah - harga_beli*jumlah AS keuntungan 
FROM penjualan INNER JOIN barang ON penjualan.id_barang=barang.id_barang WHERE status=1 AND tanggal_penjualan::date = $P{tanggal} ORDER BY tanggal_penjualan DESC]]>
	</queryString>
