	<queryString>
		<![CDATA[SELECT pj.kode_transaksi,akk.nama_akun_keuangan,ak.nama_akun,SUM(jumlah * harga_jual-(diskon/100*(jumlah * harga_jual))) - (diskon_transaksi/100*SUM(jumlah * harga_jual-(diskon/100*(jumlah * harga_jual)))) AS pendapatan_penjualan,SUM(jumlah * harga_jual-(diskon/100*(jumlah * harga_jual)))-SUM(jumlah*harga_beli)-(diskon_transaksi/100*(SUM(jumlah * harga_jual-(diskon/100*(jumlah * harga_jual)))-SUM(jumlah*harga_beli))) AS keuntungan_penjualan,pj.diskon_transaksi FROM penjualan pj INNER JOIN 
akun_keuangan akk ON pj.kode_akun_keuangan=akk.kode_akun_keuangan INNER JOIN 
akun ak ON pj.id_kasir=ak.id_akun WHERE tanggal_penjualan::date >= $P{tanggal_dari} AND tanggal_penjualan::date <= $P{tanggal_hingga} AND status=1 GROUP BY pj.kode_transaksi,akk.nama_akun_keuangan,ak.nama_akun,pj.diskon_transaksi]]>
	</queryString>
