	<queryString>
		<![CDATA[SELECT penjualan.id_barang,nama_barang,harga_beli,jenis_penjualan,harga_jual,SUM(jumlah) as jumlah,
(harga_jual*SUM(jumlah)) AS total,harga_jual * SUM(jumlah)-harga_beli*SUM(jumlah) AS keuntungan
FROM penjualan INNER JOIN barang ON penjualan.id_barang=barang.id_barang WHERE status=1 AND tanggal_penjualan::date >= $P{dari} AND tanggal_penjualan::date <= $P{ke} GROUP BY penjualan.id_barang,nama_barang,jenis_penjualan,harga_beli,harga_jual  ORDER BY SUM(jumlah) DESC]]>
	</queryString>
